start =
    expression

validchar =
    [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
      { return chars.join(""); }

spaceexpression =
    " "+ exp:expression { return exp; }

leftparen =
    "(" " "*

rightparen =
    " "* ")"

expression =
    atom
    / leftparen first:atom rest:spaceexpression* rightparen
      { return [first].concat(rest); }


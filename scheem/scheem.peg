start =
    expression

validchar =
    [0-9a-zA-Z_?!+\-=@#$%^&*/.]

atom =
    chars:validchar+
      { return chars.join(""); }

whitespace =
    " "
   /"\t"
   /"\n"
   /"\r\n"

spaceexpression =
    whitespace+ exp:expression { return exp; }

leftparen =
    "(" whitespace*

rightparen =
    whitespace* ")"

expression =
    atom
    / leftparen first:atom rest:spaceexpression* rightparen
      { return [first].concat(rest); }

